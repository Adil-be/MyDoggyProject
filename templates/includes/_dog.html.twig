<div class="card col-12 col-lg-4 col-md-6">
    {# class="card" style="width: 15rem;"> #}
    {# images carrousel #}
    {% include "includes/_image.html.twig" with {"images":dog.getImages} %}
        <div class="card-body"> <h5 class="card-title">{{dog.getName}}</h5>
        <p class="card-text">
            <strong>Description :</strong>

            <br>{{dog.getDescription}}</p>
        <p>
            <strong>Antecedant :</strong>

            <br>{{dog.getAntecedant}}</p>
        <p>
            <strong>LOF :</strong>
            {% if dog.isIsLof %}yes{% else %}no
            {% endif %}
        </p>
    </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item">
            <strong>breeds :</strong>
        </li>
        {% for breed in dog.getBreeds %}
            <li class="list-group-item">{{breed}}</li>
        {% endfor %}
    </ul>


</div>

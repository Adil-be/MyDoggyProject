{% extends "base.html.twig" %}

{% block body %}
    <h1>Register</h1>
    {{ form_start(form) }}

    {{ form_row(form.email) }}
    {{ form_row(form.username) }}
    {{ form_row(form.password) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.phoneNumber) }}
    {{ form_row(form.city) }}
    {{ form_row(form.zipCode) }}
    <button type="submit" class="btn btn-primary text-center">signIn</button>
    {{ form_end(form) }}

{% endblock body %}
